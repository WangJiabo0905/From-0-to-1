#include<iostream>
using namespace std;
int main()
{
	int* choose_sort(int* a, int index);

	int a[10]{};

	for (int i = 0; i < 10; i++)
	{
		cin >> a[i];
	}

	int *result=choose_sort(a, 10);


	for (int i = 0; i < 10; i++)
	{
		cout << result[i]<<" ";
	}


	return 0;
}

int* choose_sort(int* a, int index)//指针传递
{
	for (int i = 0; i < index - 1; i++)//次数
	{
		int max = i;
		for (int j = i+1; j < index; j++)//比较对象
		{
			if (a[max] < a[j])
			{
				max = j;
			}
		}

		if (max != i)
		{
			int temp = a[i];
			a[i] = a[max];
			a[max] = temp;
		}
	}
	return a;

}
